{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "no-restricted-syntax": 0,
        "linebreak-style": 0
    }
}
